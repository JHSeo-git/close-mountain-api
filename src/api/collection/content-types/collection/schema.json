{
  "kind": "collectionType",
  "collectionName": "collections",
  "info": {
    "singularName": "collection",
    "pluralName": "collections",
    "displayName": "Collection",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "isVerified": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "nfts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::nft.nft",
      "mappedBy": "collection"
    }
  }
}
