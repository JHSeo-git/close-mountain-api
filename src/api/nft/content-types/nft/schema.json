{
  "kind": "collectionType",
  "collectionName": "nfts",
  "info": {
    "singularName": "nft",
    "pluralName": "nfts",
    "displayName": "NFT",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "price": {
      "type": "decimal"
    },
    "description": {
      "type": "text"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "collection": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::collection.collection",
      "inversedBy": "nfts"
    },
    "tokenId": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "creator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
